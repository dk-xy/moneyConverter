{"ast":null,"code":"import { ref, watchEffect } from \"vue\";\nimport { inputValue } from './../../state.js';\nexport default {\n  __name: 'eurComponent',\n\n  setup(__props, {\n    expose\n  }) {\n    expose();\n    let euConvert = ref(0);\n    let usConvert = ref(0);\n    let isNotNull = ref(false);\n    fetch('https://cdn.jsdelivr.net/gh/fawazahmed0/currency-api@1/latest/currencies/eur.json').then(resp => resp.json()).then(eurChangeRate => {\n      console.log(eurChangeRate);\n      console.log(eurChangeRate.eur.chf);\n      const chfChangeRate = eurChangeRate.eur.chf.toFixed(2);\n      const usChangeRate = eurChangeRate.eur.usd.toFixed(2);\n      console.log(chfChangeRate);\n      console.log(usChangeRate);\n      watchEffect(() => {\n        console.log(inputValue.value);\n\n        if (inputValue.value > 0) {\n          isNotNull.value = true;\n          euConvert.value = inputValue.value * chfChangeRate;\n          usConvert.value = inputValue.value * usChangeRate;\n        } else {\n          isNotNull.value = false;\n        }\n      });\n    });\n    const __returned__ = {\n      euConvert,\n      usConvert,\n      isNotNull,\n      ref,\n      watchEffect,\n      inputValue\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n\n};","map":{"version":3,"mappings":"AACA,SACIA,GADJ,EAEIC,WAFJ,QAGO,KAHP;AAIA,SACIC,UADJ,QAEO,kBAFP;;;;;;;;IAIA,IAAIC,SAAS,GAAGH,GAAG,CAAC,CAAD,CAAnB;IACA,IAAII,SAAS,GAAGJ,GAAG,CAAC,CAAD,CAAnB;IACA,IAAIK,SAAS,GAAGL,GAAG,CAAC,KAAD,CAAnB;IAEAM,KAAK,CAAC,mFAAD,CAAL,CACKC,IADL,CACUC,IAAI,IAAIA,IAAI,CAACC,IAAL,EADlB,EAEKF,IAFL,CAEUG,aAAa,IAAI;MACnBC,OAAO,CAACC,GAAR,CAAYF,aAAZ;MACAC,OAAO,CAACC,GAAR,CAAYF,aAAa,CAACG,GAAd,CAAkBC,GAA9B;MACA,MAAMC,aAAa,GAAGL,aAAa,CAACG,GAAd,CAAkBC,GAAlB,CAAsBE,OAAtB,CAA8B,CAA9B,CAAtB;MACA,MAAMC,YAAY,GAAGP,aAAa,CAACG,GAAd,CAAkBK,GAAlB,CAAsBF,OAAtB,CAA8B,CAA9B,CAArB;MACAL,OAAO,CAACC,GAAR,CAAYG,aAAZ;MACAJ,OAAO,