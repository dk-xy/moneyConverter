{"ast":null,"code":"import { ref, watchEffect } from \"vue\";\nimport { inputValue } from './../../state.js';\nexport default {\n  __name: 'usdComponent',\n\n  setup(__props, {\n    expose\n  }) {\n    expose();\n    let euConvert = ref(0);\n    let chfConvert = ref(0);\n    let isNotNull = ref(false);\n    fetch('https://cdn.jsdelivr.net/gh/fawazahmed0/currency-api@1/latest/currencies/usd.json').then(resp => resp.json()).then(usdChangeRate => {\n      const euChangeRate = usdChangeRate.usd.eur.toFixed(2);\n      const chfChangeRate = usdChangeRate.usd.chf.toFixed(2);\n      console.log(inputValue.value);\n      watchEffect(() => {\n        console.log(inputValue.value);\n\n        if (inputValue.value > 0) {\n          isNotNull.value = true;\n          euConvert.value = inputValue.value * euChangeRate;\n          chfConvert.value = inputValue.value * chfChangeRate;\n        } else {\n          isNotNull.value = false;\n        }\n      });\n    });\n    const __returned__ = {\n      euConvert,\n      chfConvert,\n      isNotNull,\n      ref,\n      watchEffect,\n      inputValue\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n\n};","map":{"version":3,"mappings":"AACA,SACIA,GADJ,EAEIC,WAFJ,QAGO,KAHP;AAIA,SACIC,UADJ,QAEO,kBAFP;;;;;;;;IAIA,IAAIC,SAAS,GAAGH,GAAG,CAAC,CAAD,CAAnB;IACA,IAAII,UAAU,GAAGJ,GAAG,CAAC,CAAD,CAApB;IACA,IAAIK,SAAS,GAAGL,GAAG,CAAC,KAAD,CAAnB;IAEAM,KAAK,CAAC,mFAAD,CAAL,CACKC,IADL,CACUC,IAAI,IAAIA,IAAI,CAACC,IAAL,EADlB,EAEKF,IAFL,CAEUG,aAAa,IAAI;MACnB,MAAMC,YAAY,GAAGD,aAAa,CAACE,GAAd,CAAkBC,GAAlB,CAAsBC,OAAtB,CAA8B,CAA9B,CAArB;MACA,MAAMC,aAAa,GAAGL,aAAa,CAACE,GAAd,CAAkBI,GAAlB,CAAsBF,OAAtB,CAA8B,CAA9B,CAAtB;MACAG,OAAO,CAACC,GAAR,CAAYhB,UAAU,CAACiB,KAAvB;MACAlB,WAAW,CAAC,MAAM;QACdgB,OAAO,CAACC,GAAR,CAAYhB,UAAU,CAACiB,KAAvB;;QACA,IAAIjB,UAAU,CAACiB,KAAX,GAAmB,CAAvB,EAA0B;UACtBd,SAAS,CAACc,KAAV,GAAkB,IAAlB;UACAhB,SAAS,CAACgB,KAAV,GAAmBjB,UAAU,CAA