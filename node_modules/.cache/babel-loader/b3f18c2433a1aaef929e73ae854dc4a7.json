{"ast":null,"code":"import { ref, watchEffect } from \"vue\";\nimport { inputValue } from './../../state.js';\nexport default {\n  __name: 'usdComponent',\n\n  setup(__props, {\n    expose\n  }) {\n    expose();\n    let euConvert = ref(0);\n    let chfConvert = ref(0);\n    let btcConvert = ref(0);\n    let ethConvert = ref(0);\n    let isNotNull = ref(false);\n    fetch('https://cdn.jsdelivr.net/gh/fawazahmed0/currency-api@1/latest/currencies/usd.json').then(resp => resp.json()).then(usdChangeRate => {\n      const euChangeRate = usdChangeRate.usd.eur;\n      const chfChangeRate = usdChangeRate.usd.chf;\n      const btcChangeRate = usdChangeRate.usd.btc;\n      const ethChangeRate = usdChangeRate.usd.eth;\n      console.log(inputValue.value);\n      watchEffect(() => {\n        console.log(inputValue.value);\n\n        if (inputValue.value > 0) {\n          isNotNull.value = true;\n          euConvert.value = (inputValue.value * euChangeRate).toFixed(2);\n          chfConvert.value = (inputValue.value * chfChangeRate).toFixed(2);\n          btcConvert.value = (inputValue.value * btcChangeRate).toFixed(7);\n          ethConvert.value = (inputValue.value * ethChangeRate).toFixed(7);\n        } else {\n          isNotNull.value = false;\n        }\n      });\n    });\n    const __returned__ = {\n      euConvert,\n      chfConvert,\n      btcConvert,\n      ethConvert,\n      isNotNull,\n      ref,\n      watchEffect,\n      inputValue\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n\n};","map":{"version":3,"mappings":"AACA,SACIA,GADJ,EAEIC,WAFJ,QAGO,KAHP;AAIA,SACIC,UADJ,QAEO,kBAFP;;;;;;;;IAIA,IAAIC,SAAS,GAAGH,GAAG,CAAC,CAAD,CAAnB;IACA,IAAII,UAAU,GAAGJ,GAAG,CAAC,CAAD,CAApB;IACA,IAAIK,UAAU,GAAGL,GAAG,CAAC,CAAD,CAApB;IACA,IAAIM,UAAU,GAAGN,GAAG,CAAC,CAAD,CAApB;IACA,IAAIO,SAAS,GAAGP,GAAG,CAAC,KAAD,CAAnB;IAEAQ,KAAK,CAAC,mFAAD,CAAL,CACKC,IADL,CACUC,IAAI,IAAIA,IAAI,CAA