{"ast":null,"code":"import { ref, watchEffect } from \"vue\";\nimport { inputValue } from './../../state.js';\nlet euConvert = ref(0);\nlet usConvert = ref(0);\nlet isNotNull = ref(false);\nfetch('https://cdn.jsdelivr.net/gh/fawazahmed0/currency-api@1/latest/currencies/eur.json').then(resp => resp.json()).then(eurChangeRate => {\n  console.log(eurChangeRate);\n  console.log(eurChangeRate.eur.chf);\n  const chfChangeRate = eurChangeRate.eur.chf.toFixed(2);\n  const usChangeRate = eurChangeRate.eur.usd.toFixed(2);\n  console.log(chfChangeRate);\n  console.log(usChangeRate);\n  watchEffect(() => {\n    console.log(inputValue.value);\n\n    if (inputValue.value > 0) {\n      isNotNull.value = true;\n      euConvert.value = inputValue.value * chfChangeRate;\n      usConvert.value = inputValue.value * usChangeRate;\n    } else {\n      isNotNull.value = false;\n    }\n  });\n});","map":{"version":3,"mappings":"AACA,SACIA,GADJ,EAEIC,WAFJ,QAGO,KAHP;AAIA,SACIC,UADJ,QAEO,kBAFP;AAIA,IAAIC,SAAQ,GAAIH,GAAG,CAAC,CAAD,CAAnB;AACA,IAAII,SAAQ,GAAIJ,GAAG,CAAC,CAAD,CAAnB;AACA,IAAIK,SAAQ,GAAIL,GAAG,CAAC,KAAD,CAAnB;AAEAM,KAAK,CAAC,mFAAD,CAAL,CACKC,IADL,CACUC,IAAG,IAAKA,IAAI,CAACC,IAAL,EADlB,EAEKF,IAFL,CAEUG,aAAY,IAAK;EACnBC,OAAO,CAACC,GAAR,CAAYF,aAAZ;EACAC,OAAO,CAACC,GAAR,CAAYF,aAAa,CAACG,GAAd,CAAkBC,GAA9B;EACA,MAAMC,aAAY,GAAIL,aAAa,CAACG,GAAd,CAAkBC,GAAlB,CAAsBE,OAAtB,CAA8B,CAA9B,CAAtB;EACA,MAAMC,YAAW,GAAIP,aAAa,CAACG,GAAd,CAAkBK,GAAlB,CAAsBF,OAAtB,CAA8B,CAA9B,CAArB;EACAL,OAAO,CAACC,GAAR,CAAYG,aAAZ;EACAJ,OAAO,CAACC,GAAR,CAAYK,YAAZ;EAEAhB,WAAW,CAAC,MAAM;IACdU,OAAO,CAACC,GAAR,CAAYV,UAAU,CAACiB,KAAvB;;IACA,IAAIjB,UAAU,CAACiB,KAAX,GAAmB,CAAvB,EAA0B;MACtBd,SAAS,CAACc,KAAV,GAAkB,IAAlB;MACAhB,SAAS,CAACgB,KAAV,GAAmBjB,UAAU,CAACiB,KAAX,GAAmBJ,aAAtC;MACAX,SAAS,CAACe,KAAV,GAAmBjB,UAAU,CAACiB,KAAX,GAAmBF,YAAtC;IACJ,CAJA,MAIO;MACHZ,SAAS,CAACc,KAAV,GAAkB,KAAlB;IACJ;EAEH,CAVU,CAAX;AAWH,CArBL","names":["ref","watchEffect","inputValue","euConvert","usConvert","isNotNull","fetch","then","resp","json","eu